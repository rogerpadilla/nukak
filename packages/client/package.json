{
  "name": "@uql/client",
  "description": "flexible and efficient ORM, with declarative JSON syntax and smart type-safety",
  "license": "MIT",
  "version": "1.0.4",
  "type": "module",
  "main": "uql-client.min.js",
  "types": "uql-client.min.d.ts",
  "scripts": {
    "copyfiles": "copyfiles -f package.json ../../README.md ../../CHANGELOG.md dist",
    "build": "yarn clean && concurrently 'yarn:compile' 'yarn:dts' && yarn copyfiles",
    "client.watch": "NODE_ENV=production node --loader ts-node/esm node_modules/.bin/webpack",
    "start": "yarn clean && webpack --profile --bail --watch --info-verbosity verbose",
    "clean": "rimraf dist *.tsbuildinfo",
    "dts": "dts-bundle-generator --no-check -o './dist/uql-client.min.d.ts' './src/index.ts'"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.0",
    "@uql/core": "^1.0.8",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^6.12.0",
    "resolve-typescript-plugin": "^1.2.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-log": "^3.0.2"
  },
  "author": "Roger Padilla",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerpadilla/uql.git"
  },
  "bugs": {
    "url": "https://github.com/rogerpadilla/uql/issues"
  },
  "keywords": [
    "orm",
    "data-mapper",
    "persistence",
    "typescript-orm",
    "javascript-orm",
    "mariadb",
    "mariadb-orm",
    "mysql",
    "mysql-orm",
    "postgresql",
    "postgresql-orm",
    "sqlite",
    "sqlite-orm",
    "mongodb",
    "mongodb-orm",
    "entity",
    "dao",
    "transaction",
    "repository",
    "service"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3dd247e9ca2539e6bbc8877afc3ebf2da4b89a7f"
}
