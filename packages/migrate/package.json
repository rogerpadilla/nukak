{
  "name": "nukak-migrate",
  "homepage": "https://nukak.org",
  "description": "Database migration system for nukak ORM",
  "license": "MIT",
  "version": "1.8.5",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "nukak-migrate": "./cli.js"
  },
  "exports": {
    ".": "./index.js",
    "./cli": "./cli.js"
  },
  "sideEffects": false,
  "scripts": {
    "copyfiles": "copyfiles -f package.json dist",
    "build": "npm run clean && tsc -b tsconfig.build.json && npm run copyfiles",
    "start": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "dependencies": {
    "nukak": "^1.8.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.1.2",
    "typescript": "~5.4.5"
  },
  "peerDependencies": {
    "nukak": "^1.6.1"
  },
  "author": "Roger Padilla",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerpadilla/nukak.git"
  },
  "bugs": {
    "url": "https://github.com/rogerpadilla/nukak/issues"
  },
  "keywords": [
    "nukak",
    "orm",
    "migrations",
    "database-migrations",
    "schema",
    "ddl"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3243bae3502a1175eb354040642bc6e22d8a6be7"
}
