{
  "name": "@uql/core",
  "homepage": "https://uql.app",
  "description": "One Language. Frontend to Backend.",
  "license": "MIT",
  "version": "3.3.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "uql-migrate": "./dist/migrate/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dialect": "./dist/dialect/index.js",
    "./entity": "./dist/entity/index.js",
    "./querier": "./dist/querier/index.js",
    "./repository": "./dist/repository/index.js",
    "./type": "./dist/type/index.js",
    "./util": "./dist/util/index.js",
    "./namingStrategy": "./dist/namingStrategy/index.js",
    "./migrate": "./dist/migrate/index.js",
    "./options": "./dist/options.js",
    "./test": "./dist/test/index.js",
    "./mysql": "./dist/mysql/index.js",
    "./postgres": "./dist/postgres/index.js",
    "./maria": "./dist/maria/index.js",
    "./sqlite": "./dist/sqlite/index.js",
    "./mongo": "./dist/mongo/index.js",
    "./express": "./dist/express/index.js",
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js",
      "default": "./dist/browser/uql-browser.min.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "copyfiles": "copyfiles -f ../../README.md ../../CHANGELOG.md .",
    "compile.browser": "bunchee --clean false --no-dts ./src/browser/index.ts --sourcemap -o ./dist/browser/uql-browser.min.js",
    "build": "bun run clean && tsc -b tsconfig.build.json && bun run compile.browser && bun run copyfiles",
    "start": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "sqlstring": "^2.3.3",
    "sqlstring-sqlite": "^0.1.1",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "better-sqlite3": ">=9.0.0",
    "express": ">=5.0.0",
    "mariadb": ">=3.0.0",
    "mongodb": ">=6.0.0",
    "mysql2": ">=3.0.0",
    "pg": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "mariadb": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@swc/helpers": "^0.5.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/sqlstring": "^2.3.2",
    "better-sqlite3": "^12.5.0",
    "bunchee": "^6.9.1",
    "copyfiles": "^2.4.1",
    "express": "^5.2.1",
    "mariadb": "^3.4.5",
    "mongodb": "^7.0.0",
    "mysql2": "^3.16.0",
    "pg": "^8.16.3",
    "rimraf": "^6.1.2",
    "typescript": "~5.4.5"
  },
  "author": "Roger Padilla",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerpadilla/uql.git"
  },
  "bugs": {
    "url": "https://github.com/rogerpadilla/uql/issues"
  },
  "keywords": [
    "orm",
    "data-mapper",
    "persistence",
    "typescript-orm",
    "javascript-orm",
    "mariadb",
    "mariadb-orm",
    "mysql",
    "mysql-orm",
    "postgresql",
    "postgresql-orm",
    "sqlite",
    "sqlite-orm",
    "mongodb",
    "mongodb-orm",
    "entity",
    "dao",
    "transaction",
    "repository",
    "service",
    "migrations"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "be036e9602d7642190ae824c7e8c63ac71ef4231"
}
